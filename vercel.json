{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "admin-web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "^/admin/?$",
      "dest": "/admin-web/index.html"
    },
    {
      "src": "^/admin/(.*)$",
      "dest": "/admin-web/$1"
    },
    {
      "src": "^/$",
      "dest": "/public/landing.html"
    }
  ]
}

